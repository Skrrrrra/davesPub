@model IEnumerable<davesPub.Models.strongAlchogol>

@{
    Layout = "_Layout";
}

<!DOCTYPE html>
<html lang="en">
  <head>
    <link rel="stylesheet" href="/css/site.css" />
  </head>
  <body>
       <div class="inline3">
           @foreach(var item in Model)
           {
     
        <div class="itemCard">
          <div>
            <img class="imgOnCard" src="@item.LinkToImage"  />
          </div>
          <div class="titleCard">
            <a class="title">@item.title</a>
          </div>
          <div class="priceCard">
            <a class="price">@item.price</a>
          </div>
        </div>
      
           }

    


    </div>

  </body>
</html>
